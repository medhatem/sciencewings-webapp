{
  "name": "science-wings",
  "version": "1.0.0",
  "description": "ScienceWings - WebApp",
  "author": "HatemD, MostaphaA, AbdeslamN",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "start:staging": "npm run generate-swagger:staging && ng serve --configuration=staging",
    "start:local": "npm run generate-swagger:local && ng serve --configuration=local",
    "start:production": "npm run generate-swagger:production && npm run memory && ng serve --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:production": "ng build --prod --configuration=production",
    "generate-swagger:local": "node src/swagger/SwaggerUtil.local.js && ng-swagger-gen",
    "generate-swagger:staging": "node src/swagger/SwaggerUtil.staging.js && ng-swagger-gen",
    "generate-swagger:production": "node src/swagger/SwaggerUtil.production.js && ng-swagger-gen",
    "clean": "rm -rf dist/ && rm -rf node_modules && rm -rf package-lock.json",
    "clean:windows": "wsl rm -rf dist/ && wsl rm -rf node_modules && wsl rm -rf package-lock.json",
    "init-version": "node -e \"console.log('export const APP_VERSION = \\'' + process.env.npm_package_version +'\\';')\" > ./src/version.ts",
    "e2e": "ng e2e",
    "test": "ng test --browsers Chrome",
    "set-node": "nvm use 12.4.0",
    "memory": "node --max_old_space_size=4092",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "peerDependencies": {
    "webpack": "^5.0.0",
    "mini-css-extract-plugin": "2.4.3",
    "cacache": "15.3.0"
  },
  "dependencies": {
    "@angular/animations": "13.0.1",
    "@angular/cdk": "13.0.0",
    "@angular/common": "13.0.1",
    "@angular/compiler": "13.0.1",
    "@angular/core": "13.0.1",
    "@angular/forms": "13.0.1",
    "@angular/material": "13.0.0",
    "@angular/material-moment-adapter": "13.0.0",
    "@angular/platform-browser": "13.0.1",
    "@angular/platform-browser-dynamic": "13.0.1",
    "@angular/router": "13.0.1",
    "@fullcalendar/angular": "^5.10.2",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@ng-select/ng-select": "^8.1.1",
    "@ngneat/transloco": "3.1.0",
    "apexcharts": "3.29.0",
    "crypto-js": "3.3.0",
    "highlight.js": "11.3.1",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.1.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "^0.5.34",
