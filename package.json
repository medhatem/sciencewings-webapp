    "ngx-markdown": "13.0.0",
    "ngx-quill": "16.0.1",
    "ngx-toastr": "^14.2.1",
    "node-fetch": "^2.6.7",
    "perfect-scrollbar": "1.5.3",
    "quill": "1.3.7",
    "rxjs": "7.4.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.2",
    "@angular-eslint/builder": "12.6.1",
