{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true
    },
    "tslint.ignoreDefinitionFiles": false,
    "tslint.autoFixOnSave": true,
    "tslint.exclude": "**/node_modules/**/*",
    "files.exclude": {
        "**/.git": true,
        "dist/**": true,
        "logs/**": true,
        "node_modules/**": true
        },
        "typescriptHero.imports.disableImportRemovalOnOrganize": false,
        "typescriptHero.imports.insertSemicolons": true,
        "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": true,
        "typescriptHero.imports.organizeOnSave": true,
        "typescriptHero.imports.multiLineWrapThreshold": 125,
        "typescriptHero.imports.removeTrailingIndex": false,
        "typescriptHero.imports.stringQuoteStyle": "'",
        "typescript.tsdk": "node_modules\\typescript\\lib",
        "i18n-ally.localesPaths": [
            "src/assets/i18n",
            "src/app/layout/common/languages",
            "src/app/layout/common/messages",
            "src/app/mock-api/common/messages"
        ]
      }